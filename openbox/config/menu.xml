<?xml version="1.0" encoding="utf-8"?>
<openbox_menu xmlns="http://openbox.org/3.4/menu">
	<menu id="apps-accessories-menu" label="Accessories">
		<item label="Calculator">
			<action name="Execute">
				<command>
					gnome-calculator
				</command>
				<startupnotify>
					<enabled>
						yes
					</enabled>
				</startupnotify>
			</action>
		</item>
		<item label="CD Burner">
			<action name="Execute">
				<execute>
					recorder
				</execute>
			</action>
		</item>
		<item label="CD Ripper">
			<action name="Execute">
				<execute>
					asunder
				</execute>
			</action>
		</item>
		<item label="Color Picker">
			<action name="Execute">
				<execute>
					gcolor2
				</execute>
			</action>
		</item>
		<item label="File Archiver">
			<action name="Execute">
				<command>
					ark
				</command>
				<startupnotify>
					<enabled>
						yes
					</enabled>
				</startupnotify>
			</action>
		</item>
		<item label="File Manager">
			<action name="Execute">
				<execute>
					thunar
				</execute>
			</action>
		</item>
		<item label="Password Manager">
			<action name="Execute">
				<execute>
					keepassx
				</execute>
			</action>
		</item>
		<item label="Terminal">
			<action name="Execute">
				<execute>
					term
				</execute>
			</action>
		</item>
	</menu>
	<menu id="apps-com-menu" label="Communication">
		<item label="Email">
			<action name="Execute">
				<execute>
					claws-mail
				</execute>
			</action>
		</item>
		<item label="Instant Messenger">
			<action name="Execute">
				<execute>
					pidgin
				</execute>
			</action>
		</item>
		<item label="Microblogging Client">
			<action name="Execute">
				<execute>
					pino
				</execute>
			</action>
		</item>
		<item label="IRC  Chat">
			<action name="Execute">
				<execute>
					xchat
				</execute>
			</action>
		</item>
	</menu>
	<menu id="apps-editors-menu" label="Office">
		<item label="Document Viewer">
			<action name="Execute">
				<execute>
					epdfview
				</execute>
			</action>
		</item>
		<item label="Word Processor">
			<action name="Execute">
				<execute>
					abiword
				</execute>
			</action>
		</item>
		<item label="Text Editor">
			<action name="Execute">
				<command>
					geany
				</command>
				<startupnotify>
					<enabled>
						yes
					</enabled>
					<wmclass>
						Geany
					</wmclass>
				</startupnotify>
			</action>
		</item>
		<item label="Wiki">
			<action name="Execute">
				<execute>
					zim
				</execute>
			</action>
		</item>
	</menu>
	<menu id="apps-net-menu" label="Internet">
		<item label="Bittorrent Client">
			<action name="Execute">
				<execute>
					punchfw.py transmission
				</execute>
			</action>
		</item>
		<item label="Feed Reader">
			<action name="Execute">
				<execute>
					liferea
				</execute>
			</action>
		</item>
		<item label="Netcat">
			<action name="Execute">
				<execute>
					gtknetcat
				</execute>
			</action>
		</item>
		<item label="Web Browser">
			<action name="Execute">
				<execute>
					/usr/bin/firefox
				</execute>
			</action>
		</item>
	</menu>
	<menu id="apps-multimedia-menu" label="Media">
		<item label="Video Player">
			<action name="Execute">
				<command>
					gmplayer
				</command>
				<startupnotify>
					<enabled>
						yes
					</enabled>
					<wmclass>
						MPlayer
					</wmclass>
				</startupnotify>
			</action>
		</item>
		<item label="Music Player">
			<action name="Execute">
				<command>
					totem
				</command>
				<startupnotify>
					<enabled>
						yes
					</enabled>
				</startupnotify>
			</action>
		</item>
		<item label="Image Editor">
			<action name="Execute">
				<execute>
					gimp
				</execute>
			</action>
		</item>
		<item label="Image Viewer">
			<action name="Execute">
				<execute>
					mirage
				</execute>
			</action>
		</item>
		<item label="Vector Editor">
			<action name="Execute">
				<execute>
					inkscape
				</execute>
			</action>
		</item>
	</menu>
	<menu id="apps-security-menu" label="Security">
		<item label="Metasploit">
			<action name="Execute">
				<execute>
					/opt/metasploit/msfgui
				</execute>
			</action>
		</item>
		<item label="Nmap">
			<action name="Execute">
				<execute>
					gksudo zenmap
				</execute>
			</action>
		</item>
		<item label="Ophcrack">
			<action name="Execute">
				<execute>
					ophcrack
				</execute>
			</action>
		</item>
		<item label="Truecrypt">
			<action name="Execute">
				<execute>
					truecrypt
				</execute>
			</action>
		</item>
		<item label="Wireshark">
			<action name="Execute">
				<execute>
					wireshark
				</execute>
			</action>
		</item>
	</menu>
	<menu id="settings-menu" label="Settings">
		<item label="Gconf Editor">
			<action name="Execute">
				<execute>
					gconf-editor
				</execute>
			</action>
		</item>
		<item label="Keybindings">
			<action name="Execute">
				<execute>
					obkey
				</execute>
			</action>
		</item>
		<item label="Menu (Gnome)">
			<action name="Execute">
				<execute>
					gmenu-simple-editor
				</execute>
			</action>
		</item>
		<item label="Menu (Openbox)">
			<action name="Execute">
				<execute>
					obmenu
				</execute>
			</action>
		</item>
		<item label="Openbox">
			<action name="Execute">
				<execute>
					obconf
				</execute>
			</action>
		</item>
		<item label="Screensaver">
			<action name="Execute">
				<execute>
					gnome-screensaver-preferences
				</execute>
			</action>
		</item>
		<item label="Theme">
			<action name="Execute">
				<execute>
					lxappearance
				</execute>
			</action>
		</item>
		<item label="Wallpaper">
			<action name="Execute">
				<execute>
					nitrogen
				</execute>
			</action>
		</item>
		<separator/>
		<item label="Reload WM">
			<action name="Reconfigure"/>
		</item>
	</menu>
	<menu id="system-menu" label="System">
		<item label="Disk Utility">
			<action name="Execute">
				<execute>
					palimpsest
				</execute>
			</action>
		</item>
		<item label="Manage Cups Printers">
			<action name="Execute">
				<command>
					xdg-open http://localhost:631/
				</command>
				<startupnotify>
					<enabled>
						no
					</enabled>
					<icon>
						cups
					</icon>
				</startupnotify>
			</action>
		</item>
		<item label="System Monitor">
			<action name="Execute">
				<execute>
					lxtask
				</execute>
			</action>
		</item>
	</menu>
	<menu id="session-menu" label="Session">
		<item label="Lock">
			<action name="Execute">
				<execute>
					xsession lock
				</execute>
			</action>
		</item>
		<item label="Sleep">
			<action name="Execute">
				<execute>
					xsession suspend
				</execute>
			</action>
		</item>
		<separator/>
		<item label="Logout">
			<action name="Execute">
				<execute>
					xsession logout
				</execute>
			</action>
		</item>
		<item label="Restart">
			<action name="Execute">
				<execute>
					xsession restart
				</execute>
			</action>
		</item>
		<item label="Shutdown">
			<action name="Execute">
				<execute>
					xsession shutdown
				</execute>
			</action>
		</item>
	</menu>
	<menu execute="/usr/local/bin/xdg-menu" id="xdg-menu" label="All"/>
	<menu id="root-menu" label="Openbox 3">
		<separator label="Applications"/>
		<menu id="apps-accessories-menu"/>
		<menu id="apps-com-menu"/>
		<menu id="apps-editors-menu"/>
		<menu id="apps-net-menu"/>
		<menu id="apps-multimedia-menu"/>
		<menu id="apps-security-menu"/>
		<separator/>
		<menu id="xdg-menu"/>
		<separator label="System"/>
		<menu id="settings-menu"/>
		<menu id="system-menu"/>
		<separator/>
		<menu id="session-menu"/>
	</menu>
</openbox_menu>
